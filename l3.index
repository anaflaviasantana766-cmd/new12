      font-weight: 400;
      font-size: 18px;
      color: #ffffff;
    }
    .offer-details p {
      margin: 0 0 10px;
    }
    .book-columns {
      padding: 20px;
      text-align: center;
    }
    .book-columns table {
      width: 100%;
      border-collapse: collapse;
    }
    .book-columns td {
      width: 33.33%;
      padding: 10px;
      vertical-align: top;
    }
    .book-columns img {
      max-width: 100%;
      height: auto;
      display: block;
      margin: 0 auto;
    }
    .bottom-link {
      padding: 20px;
      text-align: center;
      font-family: 'Roboto', sans-serif;
      font-weight: 400;
      font-size: 18px;
      color: #ffffff;
      background-color: #f3971a;
    }
    .bottom-link a {
      color: #ffffff;
      text-decoration: underline;
    }
    .content {
