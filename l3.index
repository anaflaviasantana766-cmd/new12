      text-align: center;
      color: #ffffff;
    }
    .footer {
      padding: 20px;
      font-size: 12px;
      color: #ffffff;
      text-align: center;
    }
    .footer a {
      color: #ffffff;
      text-decoration: underline;
    }
    @media only screen and (max-width: 600px) {
      .container {
        width: 100%;
        border-radius: 15px;
      }
      .top-bar {
        border-top-left-radius: 15px;
        border-top-right-radius: 15px;
      }
      .top-bar span {
        font-size: 14px;
        margin: 0 10px;
      }
