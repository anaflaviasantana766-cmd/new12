      text-align: center;
      line-height: 71px;
      border-top-left-radius: 25px;
      border-top-right-radius: 25px;
    }
    .top-bar span {
      font-family: 'Roboto', sans-serif;
      font-weight: 700;
      font-size: 16px;
      margin: 0 15px;
    }
